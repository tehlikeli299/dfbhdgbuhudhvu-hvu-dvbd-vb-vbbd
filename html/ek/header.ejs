<style>
.rainbow {
  text-align: left;
  font-size: 16px;
  animation: colorRotate 4s linear 0.1s infinite;
}

@keyframes colorRotate {
  from {
    color: #6666ff;
  }
  10% {
    color: #0099ff;
  }
  50% {
    color: #00ff00;
  }
  75% {
    color: #ff3399;
  }
  100% {
    color: #6666ff;
  }
}
.rainbowicon {
  text-align: left;
  font-size: 16px;
  animation: colorRotate2 4s linear 0.1s infinite;
}

@keyframes colorRotate2 {
  from {
    border: 4px solid #6666ff;
  }
  10% {
    border: 4px solid #0099ff;
  }
  50% {
    border: 4px solid #00ff00;
  }
  75% {
    border: 4px solid #ff3399;
  }
  100% {
     border: 4px solid #6666ff;
  }
} 

.bg {
  background-color:#212529;
  padding: 0;
}
 </style>
